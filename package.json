{
  "name": "flightbook-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/db/db-config-migrations.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- migration:generate ./src/db/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "expose": "lt --port 8282",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:it": "jest --config ./test/it/it.jest.json --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json  --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.28.0",
    "@aws-sdk/s3-request-presigner": "^3.94.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/geojson": "^7946.0.10",
    "@types/minimatch": "^5.1.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.5.3",
    "firebase-admin": "^12.1.1",
    "google-auth-library": "^8.0.2",
    "helmet": "^6.0.1",
    "jwks-rsa": "^3.2.0",
    "moment": "^2.29.4",
    "multer-s3": "^2.10.0",
    "nestjs-i18n": "^10.0.0",
    "nodemailer": "^6.7.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "stripe": "^18.5.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.9",
    "@testcontainers/postgresql": "^10.8.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/stripe-v3": "^3.1.27",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.3.1",
    "prettier": "^3.0.0",
    "supertest": "^6.2.2",
    "testcontainers": "^10.8.1",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
